{
  "name": "tencentcloud-monitor-grafana-app",
  "private": false,
  "version": "2.0.1",
  "description": "Tencent Cloud Monitor plugin for Grafana",
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "tencentcloud",
    "tencentcloudmonitor",
    "grafana",
    "plugins"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TencentCloud/tencentcloud-monitor-grafana-app.git"
  },
  "author": "Tencent Cloud",
  "license": "Apache-2.0",
  "scripts": {
    "build": "./bin/build-prod.sh",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "prettier": "prettier --config .prettierrc.js --write 'src/**/*.{tsx,ts,jsx,js,css,vue,json}'",
    "eslint": "eslint -c .eslintrc --fix 'src/**/*.{tsx,ts,jsx,js,css,vue,json}'",
    "sign": "grafana-toolkit plugin:sign"
  },
  "bugs": {
    "url": "https://github.com/TencentCloud/tencentcloud-monitor-grafana-app/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@grafana/data": "^7.4.5",
    "@grafana/runtime": "^7.4.5",
    "@grafana/toolkit": "^7.4.5",
    "@types/angular": "^1.6.6",
    "@types/grafana": "https://git@github.com/CorpGlory/types-grafana.git",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.123",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "5.1.2",
    "eslint": "^7.21.0",
    "eslint-config-alloy": "^3.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.8",
    "jest": "^24.8.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "angular": "^1.8.2",
    "crypto-js": "^3.1.9-1",
    "dot-qs": "^0.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css}": [
      "eslint -c .eslintrc --fix",
      "prettier --config .prettierrc.js --write"
    ]
  }
}
